{
  "name": "pomodoro",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "bump": "node scripts/bump-version.js",
    "prebuild": "yarn bump -- patch",
    "dev": "node -r dotenv/config node_modules/.bin/expo start",
    "build:android:local": "yarn prebuild && eas build --platform android --profile development --local",
    "build:ios:local": "yarn prebuild && eas build --platform ios --profile development --local",
    "build:android:preview": "yarn prebuild && eas build --platform android --profile preview",
    "build:ios:preview": "yarn prebuild && eas build --platform ios --profile preview",
    "build:android:prod": "yarn prebuild && eas build --platform android --profile production",
    "build:ios:prod": "yarn prebuild && eas build --platform ios --profile production"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@types/react-native-svg-charts": "^5.0.16",
    "dotenv": "^16.5.0",
    "expo": "~53.0.11",
    "expo-build-properties": "^0.14.6",
    "expo-constants": "^17.1.6",
    "expo-dev-client": "~5.2.0",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "^7.1.5",
    "expo-router": "^5.1.0",
    "expo-splash-screen": "^0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "^5.0.8",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-google-mobile-ads": "^15.4.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-webview": "13.13.5",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "private": true
}
